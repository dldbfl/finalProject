package com.dlms.request;

import java.util.List;

import org.springframework.web.multipart.MultipartFile;

public class FoundedReviewImgRequest {

	
	private List<MultipartFile> addFiles;
	private List<Integer> removefile;
	
	public FoundedReviewImgRequest() {}
	
	
	public List<MultipartFile> getAddFiles() {
		return addFiles;
	}

	public void setAddFiles(List<MultipartFile> addFiles) {
		this.addFiles = addFiles;
	}


	public List<Integer> getRemovefile() {
		return removefile;
	}


	public void setRemovefile(List<Integer> removefile) {
		this.removefile = removefile;
	}


	@Override
	public String toString() {
		return "FoundedReviewImgRequest [addFiles=" + addFiles + ", removefile=" + removefile + "]";
	}


	
	
}
