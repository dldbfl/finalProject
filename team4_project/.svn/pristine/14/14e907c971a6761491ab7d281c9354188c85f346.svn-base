package com.dlms.dto;

import java.util.Date;
import java.util.List;
/**
 * 
 * @사용목적		:	강의출석부
 * @작성자			:	이누리
 * @작성날짜     	:	2020. 6. 4.
 * @마지막수정자	:	이누리
 * @마지막수정일	:	2020. 7. 01.오후 2:43:13
 * @see				:	-
 *
 */
public class Lecture_attendVO {

	private int lecture_attend_no;				// 강의 출석번호
	private int lecture_data_no;				// 강의 동영상
	private String student_id; 					// 관리자
	private Date lecture_attend_date; 			// 출석일
	private String lecture_attend_status;		// 출석상태
	private String professor_id;				//교수 아이디

	private int lecture_attend_daycount; 	    // 현재 출석일 수 
	private int lecture_attend_alldaycount;     // 총 출석일 수 
	
	public Lecture_attendVO() {}

	public int getLecture_attend_no() {
		return lecture_attend_no;
	}

	public void setLecture_attend_no(int lecture_attend_no) {
		this.lecture_attend_no = lecture_attend_no;
	}


	public String getStudent_id() {
		return student_id;
	}

	public void setStudent_id(String student_id) {
		this.student_id = student_id;
	}

	public Date getLecture_attend_date() {
		return lecture_attend_date;
	}

	public void setLecture_attend_date(Date lecture_attend_date) {
		this.lecture_attend_date = lecture_attend_date;
	}

	public String getLecture_attend_status() {
		return lecture_attend_status;
	}

	public void setLecture_attend_status(String lecture_attend_status) {
		this.lecture_attend_status = lecture_attend_status;
	}

	public int getLecture_attend_daycount() {
		return lecture_attend_daycount;
	}

	public void setLecture_attend_daycount(int lecture_attend_daycount) {
		this.lecture_attend_daycount = lecture_attend_daycount;
	}

	public int getLecture_attend_alldaycount() {
		return lecture_attend_alldaycount;
	}

	public void setLecture_attend_alldaycount(int lecture_attend_alldaycount) {
		this.lecture_attend_alldaycount = lecture_attend_alldaycount;
	}

	public int getLecture_data_no() {
		return lecture_data_no;
	}

	public void setLecture_data_no(int lecture_data_no) {
		this.lecture_data_no = lecture_data_no;
	}


	public String getProfessor_id() {
		return professor_id;
	}

	public void setProfessor_id(String professor_id) {
		this.professor_id = professor_id;
	}

	@Override
	public String toString() {
		return "Lecture_attendVO [lecture_attend_no=" + lecture_attend_no + ", lecture_data_no=" + lecture_data_no
				+ ", student_id=" + student_id + ", lecture_attend_date=" + lecture_attend_date
				+ ", lecture_attend_status=" + lecture_attend_status + ", professor_id=" + professor_id
				+ ", lecture_attend_daycount=" + lecture_attend_daycount + ", lecture_attend_alldaycount="
				+ lecture_attend_alldaycount + "]";
	}

	public Lecture_attendVO(int lecture_attend_no, int lecture_data_no, String student_id, Date lecture_attend_date,
			String lecture_attend_status, String professor_id, int lecture_attend_daycount,
			int lecture_attend_alldaycount) {
		super();
		this.lecture_attend_no = lecture_attend_no;
		this.lecture_data_no = lecture_data_no;
		this.student_id = student_id;
		this.lecture_attend_date = lecture_attend_date;
		this.lecture_attend_status = lecture_attend_status;
		this.professor_id = professor_id;
		this.lecture_attend_daycount = lecture_attend_daycount;
		this.lecture_attend_alldaycount = lecture_attend_alldaycount;
	}

	
	
}
